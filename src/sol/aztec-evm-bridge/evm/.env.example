# Cronos EVM Testnet Deployment Configuration

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=

# Cronos Testnet RPC URL
# Default: https://evm-t3.cronos.org
RPC_URL=https://evm-t3.cronos.org

# Permit2 Contract Address on Cronos Testnet
# This is the Universal Permit2 contract address
PERMIT2=

# Aztec Network Configuration
# Aztec Inbox contract address
AZTEC_INBOX=

# Aztec Outbox contract address
AZTEC_OUTBOX=

# Anchor State Registry contract address
ANCHOR_STATE_REGISTRY=

# Aztec Gateway 7683 contract address (bytes32)
AZTEC_GATEWAY_7683=

# Optional: Deploy Test Token (true/false)
DEPLOY_TEST_TOKEN=false

# Optional: Deploy L2 Gateway (true/false)
DEPLOY_L2_GATEWAY=true

# Optional: Deploy Forwarder (true/false)
DEPLOY_FORWARDER=true

# Optional: Existing L2 Gateway Address (if not deploying new one)
L2_GATEWAY_ADDRESS=
